{
    "compiler-nvim-solution-toml": {
        "prefix": "compiler-nvim-solution-toml",
        "description": "Template for your compiler.nvim `.solution.toml` file.",
        "body": [
            "[${2:AnyNameYouWant}]",
            "entry_point = \"${3:./main.c}\"",
            "output = \"${4:./bin/myprogram}\"",
            "",
            "[SOLUTION]",
            "executable = \"${5:./bin/myprogram}\"$0"
        ]
    }
}
